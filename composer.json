{
    "name": "norbybaru/modularize",
    "description": "Generate modular structure files for laravel",
    "homepage": "https://github.com/norbybaru/modularize",
    "keywords": ["laravel", "modular", "modules", "module", "structure", "modular", "laravel-modular", "modularize"],
    "require": {
        "php": "^8.0",
        "illuminate/console": "^10.13",
        "illuminate/support": "^8.0|^9.52|^10.0"
    },
    "license": "MIT",
    "version": "2.0",
    "authors": [
        {
            "name": "Norby Baruani",
            "email": "norbybaru@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "NorbyBaru\\Modularize\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NorbyBaru\\Modularize\\ModularizeServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "fmt": "./vendor/bin/pint -v",
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "laravel/framework": "^10.13",
        "laravel/pint": "^1.10"
    }
}
